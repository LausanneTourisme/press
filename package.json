    "prettier-plugin-tailwindcss": "^0.6.11",
    "sass": "^1.83.4",
    "svelte": "^5.19.5",
    "svelte-check": "^4.1.4",
    "tailwindcss": "^4.0.1",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.22.0",
    "vite": "^6.0.11"
  },
  "dependencies": {
    "@beyonk/gdpr-cookie-consent-banner": "^12.1.4",
